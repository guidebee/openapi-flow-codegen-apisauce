{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export interface {{{name}}}{{>extends}} {
    {{#each properties}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{>isReadOnly}}{{{name}}}{{>isRequired}}: {{>type parent=../name}};
    {{/each}}
}
{{#if enums}}

// FIXME: namespaces in flow?! no idea
export namespace {{{name}}} {

    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    export enum {{{name}}} {
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}

}
{{/if}}
