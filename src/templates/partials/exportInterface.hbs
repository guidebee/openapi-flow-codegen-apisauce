{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export interface {{{name}}}{{>extends}} {
    {{#each properties}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{>isReadOnly}}{{{name}}}{{>isRequired}}: {{>type parent=../name}};
    {{/each}}
}
{{#if enums}}

/**
 * Flow-way of handling enums
 */
export const {{{name}}} = {

    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{{name}}}: {
        {{#each enum}}
        {{{name}}}: {{{value}}},
        {{/each}}
    },

    {{/each}}

}
{{/if}}
